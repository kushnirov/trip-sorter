{
    "name": "kushnirov/trip-sorter",
    "description": "Sort tickets and return full trip",
    "type": "project",
    "authors": [
        {
            "name": "Sergey Kushnirov",
            "email": "sergey@kushnirov.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "example": "php ./example.php",
        "test": "./vendor/bin/phpunit --configuration ./phpunit.xml",
        "phpcs": "./vendor/bin/phpcs --standard=./phpcs.xml"
    }
}
